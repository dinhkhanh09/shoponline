@model List<ShopOnline.Models.category>

@foreach (var i in Model)
{
    <div class="para-an">
        <h3><a href="@ViewBag.meta/@i.meta">@i.name</a></h3>
    </div>
    <div class="lady-in">
        @{Html.RenderAction("getProduct", "Default", new { id = @i.id, metatitle = @ViewBag.metatitle });}
    </div>
}
